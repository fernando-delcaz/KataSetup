# This file is the top-most EditorConfig file
root = true

# All Files
[*]
charset = utf-8
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = crlf

# Solution Files
[*.sln]
indent_style = tab


# YAML
[*.yml]
indent_style = space
indent_size = 2